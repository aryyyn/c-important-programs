//overloading function template
//wap to find the max and min of 2 numbers

#include <iostream>
using namespace std;

template <typename T>
T maximum(T t1, T t2)
{
    if (t1 > t2)
    {
        cout<<endl<<"t1 is greater than t2";
        
    }
    else
    {
        cout<<endl<<"t2 is greater than t1";
    }
    return 0;
}

template <typename T2>
T2 maximum(T2 t3, T2 t4, T2 t5)
{
    if (t3>t4>t5)
    {
        cout<<endl<<"t3 is greater than t4 and t5";
    }
    else if(t4>t3>t5)
    {
        cout<<endl<<"T4 is greater than t3 and t5";
    }
    else
    cout<<endl<<"t5 is the greatest";
}

int main()

{   
    maximum(2,3);
    maximum(2,4,5);

    
    return 0;
}
